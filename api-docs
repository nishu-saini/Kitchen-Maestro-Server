paths:
  /api/recipe/ingredients/:
    get:
      operationId: recipe_ingredients_list
      description: Manage ingredients in the database
      parameters:
      - in: query
        name: assigned_only
        schema:
          type: integer
          enum:
          - 0
          - 1
        description: Filter by items assigned to recipes
      tags:
      - recipe
      security:
      - tokenAuth: []
      responses:
        '200':
          content:
            application/json:
              schema:
                type: array
                items:
                  $ref: '#/components/schemas/Ingredient'
          description: ''
